{
  "templates":
    [
      {
        "match": { "config": "mpl" },
        "template": "/usr/bin/time -v bin/<bench>.<config>.bin @mpl <procs?procs {}> set-affinity affinity-base 1 -- <args?{} ><args1?{}>"
      },
      {
        "match": { "config": "mlton" },
        "template": "/usr/bin/time -v bin/<bench>.<config>.bin <args?{} ><args1?{}>"
      }
    ],

  "globals":
    {
      "procs": ["1", "10", "20", "30", "40", "50", "60", "70"],
      "config": ["mpl"],
      "cwd": ["ml"]
    },

  "specs":
    [
      {
        "tag": ["fib"],
        "bench": ["fib"],
        "args": ["-N 42"],
        "args1": ["-repeat 10 -warmup 5"]
      }
    ]
}
